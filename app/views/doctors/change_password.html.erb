<%= provide(:title, "Edit Profile") %>
<%= render 'doctors/sidebar' %>

<div class="col-xs-8">
  <div class="row">
    <div class="col-xs-12">
      <div class="panel panel-default">
        <div class="panel-heading"><h4>Change Password</h4></div>
        <div class="panel-body">
          <%= render 'shared/error_messages', object: @doctor %>


          <%= form_tag(doctor_update_password_path, method: :post, html: { :multipart => :true, class: "form-horizontal", role: "form" }) do %>
            <%= render 'shared/password_change_form' %>
            <%= submit_tag 'Change', class: 'btn btn-success' %>

          <% end %>

        </div>
      </div>
    </div>
  </div>
</div>

</div>





