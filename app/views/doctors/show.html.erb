<%= provide(:title, "My Profile") %>
<%= render 'sidebar' %>

<%= render 'shared_show' %>

<div class="col-xs-2">
  <div class="row">
    <div class="col-xs-12">
      <div class="panel panel-default">
        <div class="panel-heading"><h4>Options</h4></div>
        <div class="panel-body">
        <%= link_to 'Edit Profile', edit_doctor_path(@doctor) %>
        </div>
      </div>
    </div>
  </div>
</div>

<div class="col-xs-3">
</div>
<div class="col-xs-7">
  <div class="row">
    <div class="col-xs-10">
      <div class="panel panel-default">
        <div class="panel-heading"><h4>Personal Information</h4></div>
        <div class="panel-body">
          <b>Email:</b> <%= @doctor.email %><br/>
          <b>Phone:</b> <%= @doctor.phone_number %><br/>
        </div>
      </div>
    </div>
  </div>
</div>

</div>


