<%= provide(:title, 'Admin Panel') %>
<%= render 'admins/header' %>

<div class="row">
  <div class="col-xs-8 col-xs-offset-2">
    <div class="row">
      <div class="col-xs-12">
        <div class="panel panel-default">
          <div class="panel-heading"><h4>Today's Appointments</h4></div>
          <div class="panel-body panel-inverse">
            <div class="day_appointments">
            <%= render 'admins/appointments/appointments_table', appointments: @appointments %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
