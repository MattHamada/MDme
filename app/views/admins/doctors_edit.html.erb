<%= provide(:title, "Edit Appointment") %>
<%= render 'admins/sidebar' %>

<div class="col-xs-8">
  <div class="row">
    <div class="col-xs-12">
      <div class="panel panel-default">
        <div class="panel-heading"><h4>Edit Doctor</h4></div>
        <div class="panel-body">

          <%= form_for(@doctor, method: :put, html: { :multipart => :true, class: "form-horizontal", role: "form" }) do |f| %>
              <%= render 'doctors/new_edit_form', f: f %>

              <%= f.submit 'Update', class: 'btn btn-success' %>

          <% end %> <br/>
          <%= link_to 'Delete Doctor', doctor_path(@doctor), method: :delete %>

        </div>
      </div>
    </div>
  </div>
</div>

</div>





