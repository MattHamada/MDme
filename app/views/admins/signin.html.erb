<%= provide(:title, "Admin Sign In") %>


<div class='container'>
  <%= render 'shared/flash' %>
  <div class='row'>
    <div class='col-md-10' >
      <h1>Admin Sign In</h1>
      <%= form_for(:session, url:sessions_path, html: { class: 'form form-horizontal form-group', role: 'form' }) do |f| %>
          <%= render '/shared/signin', f: f %>
      <% end %>
    </div>
  </div>

