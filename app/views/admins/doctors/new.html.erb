<%= provide(:title, "New Doctor | ") %>
<div class="container">
  <%= render 'admins/doctors/sub_menu' %>
  <div class="row">
    <div class="col-md-10 col-md-offset-1">
      <div class="panel panel-default">
        <div class="panel-heading"><h4>Add Doctor</h4></div>
        <div class="panel-body panel-inverse">

          <%= form_for(@doctor, url: url_for(controller: 'admins/doctors', action: 'create'), html: { :multipart => :true, class: "form-group", role: "form" }) do |f| %>
            <%= render 'doctors/new_edit_form', f: f %>

            <div class="col-xs-6 col-xs-offset-3" style="text-align: center;">
              <%= f.submit 'Create', class: 'btn btn-success' %>
            </div>
          <% end %>

        </div>
      </div>
    </div>
  </div>
</div>




