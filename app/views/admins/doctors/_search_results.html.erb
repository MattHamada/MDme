<% if doctors.empty? %>
  <div class="alert alert-warning">
    No records found
  </div>
<% else %>
  <table class='table table-striped table-bordered table-condensed table-inverse'>
    <thread>
      <tr>
        <th>#</th>
        <th>Name</th>
        <th>Department</th>
      </tr>
      <tbody>
      <% doctors.each_with_index  do |doctor, index| %>
        <tr>
          <td class="popup-links"><%= link_to("#{index+1}", admin_doctor_path(@admin, doctor)) %></td>
          <td><%= doctor.full_name %></td>
          <td><%= doctor.department_name %></td>
        </tr>
      <% end %>
      </tbody>
    </thread>
  </table>
<% end %>