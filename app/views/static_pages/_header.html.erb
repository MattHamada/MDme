<div class="container" id="main-content">
  <%= render 'shared/flash' %>
  <div class="row">
    <div class="col-md-5">
      <a  href="#"><img src="<%= asset_path( 'MDME_banner.png' ) %>" /> </a>
    </div>
  </div>
  <div class="row-fluid">
    <div id="static-links">
      <div class="col-md-1 col-md-offset-7">
        <%= link_to 'about us', about_path %>
      </div>
      <div class="col-md-1">
        <a href="#">services</a>
      </div>
      <div class="col-md-1">
        <%= link_to 'contact', contact_path %>
      </div>
      <div class="col-md-1">
        <a href="#">resources</a>
      </div>
      <div class="col-md-1">
        <% if patient_signed_in? %>
          <%= link_to 'sign out', signout_path, method: :delete %>
        <% else %>
          <a href="/signin">sign in</a>
        <% end %>
      </div>
    </div>
  </div>
  <br/>
  <hr id="nav-break"/>

