<div class='form-group'>
  <div class='col-md-3'>
    <%= f.label :email %>
    <%= f.text_field :email, class: 'form-control' %>
  </div>
</div>
<div class='form-group'>
  <div class='col-md-3'>
    <%= f.label :password %>
    <%= f.password_field :password, class: 'form-control' %>
  </div>
</div>
<%= f.submit 'Sign in', class: 'btn btn-lg btn-primary' %>