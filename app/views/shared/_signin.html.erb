<div class='form-group'>
  <div class='col-md-6 col-md-offset-3'>
    <%= f.text_field :email, class: 'form-control signin_form' %>
  </div>
</div>
<div class='form-group'>
  <div class='col-md-6 col-md-offset-3'>
    <%= f.password_field :password, class: 'form-control signin_form' %>
  </div>
</div>
<div class="form-group">
  <div style="text-align: center">
    <%= f.submit 'SIGN IN', class: 'btn btn-lg btn-primary signin-button'  %>
  </div>
</div>