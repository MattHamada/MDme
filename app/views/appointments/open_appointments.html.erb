
<table class='table table-striped table-bordered table-hover'>
  <thread>
    <tr>
      <th>#</th>
      <th>Time</th>

    </tr>
    <tbody>
    <% @open_times.each_with_index do |time, index| %>
        <tr>
          <td><%="#{index}"%></td>
          <td><%= time %></td>

        </tr>
    <% end %>
    </tbody>
  </thread>
</table>

