
<table class='table table-striped table-bordered table-hover'>
  <thread>
    <tr>
      <th>Time</th>

    </tr>
    <tbody>
    <% @open_times.each do |time| %>
        <% time %>
        <tr>
          <td><%= link_to time, new_request_appointment_path(@patient, date: @date, time: time, doctor: @doctor.id) %></td>
        </tr>
    <% end %>
    </tbody>
  </thread>
</table>

