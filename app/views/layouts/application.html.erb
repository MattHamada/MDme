<!DOCTYPE html>
<html>
<head>
  <%= favicon_link_tag %>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>

  <% if request.variant.include? :mobile %>
  <%= stylesheet_link_tag 'mobile', media: 'all', "data-turbolinks-track" => true %>
    <meta name="viewport" content="width=device-width, initial-scale=1">
  <%= javascript_include_tag 'mobile' %>
    <meta name="viewport" content="width=device-width, initial-scale=1">


<% end %>
  <%= csrf_meta_tags %>


  <meta charset="utf-8">
  <meta name="description" content="">
  <meta name="author" content="">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">

  <title><%= yield :title %> MD & ME</title>

</head>
<body>

<!--&lt;!&ndash;<script>&ndash;&gt;-->
<!--//  (function(){var s=document.createElement("script");s.onload=function(){bootlint.showLintReportForCurrentDocument([]);};s.src="https://maxcdn.bootstrapcdn.com/bootlint/latest/bootlint.min.js";document.body.appendChild(s)})();-->
<!--&lt;!&ndash;</script>&ndash;&gt;-->

<% if request.subdomain == 'www' || request.subdomain.blank?  %>
  <% unless   !request.variant.include? :mobile %>
    <% render 'static_pages/header' %>
  <% end %>
<% end %>

<% if request.variant.include? :mobile %>
  <%= render 'patients/mobile/header' %>
<% end %>

  <%= yield %>
<% unless request.variant.include? :mobile %>
  <%= render 'layouts/footer' %>
<% end %>
</body>
</html>
