<div id="footer">
  <div class="container-fluid">
    <div class="row">
      <div class="static-pages-menu-links">
        <div class="col-sm-3">
          <%= link_to 'home', root_path %>
        </div>
        <div class="col-sm-3">
          <a href="#/about">about</a>
        </div>
        <div class="col-sm-3">
          <%= link_to 'contact', contact_path %>
        </div>
        <% if @current_patient.nil? %>
          <div  ng-hide='loggedIn()' class="col-sm-3">
            <%= link_to 'sign in', signin_path %>
          </div>
        <% else %>
          <div ng-show="loggedIn()" class="col-sm-3">
            <a href="#/patients">my account</a>
          </div>
        <% end %>
      </div>
    </div>
    <div class="row">
      <div class="col-md-12">
        <% debug(params) if Rails.env.development? %>
      </div>
    </div>
  </div>
</div>


