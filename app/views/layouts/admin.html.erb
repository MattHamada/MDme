<!DOCTYPE html>
<%# if request.subdomain == 'admin' %>
<%# <html ng-app="mdme-admin">    %>
<%# else %>
<%# <html ng-app="mdme">  %>
<%# end %>
<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta http-equiv="x-ua-compatible" content="ie=edge">

  <meta name="description" content="MDme">
  <meta name="author" content="MDme">
  <%= favicon_link_tag %>

  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <link href="https://file.myfontastic.com/ydXhJAMpKMFatMxXYNTBFK/icons.css" rel="stylesheet">

  <%= csrf_meta_tags %>


  <title><%= yield :title %> MD & ME</title>

</head>
  <body style="padding-top: 50px;">
    <%= render 'admins/header' %>
    <div class="container-fluid">
      <div class="row">
        <%= render 'admins/sidebar' %>
        <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
          <%= yield %>
        </div>
      </div>
    </div>
  </body>
</html>
